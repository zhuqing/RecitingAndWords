<template name="floatplayer">
<view class='player-position' style='bottom: {{playerItem.bottom}}px;right: {{playerItem.right}}px;'>
<canvas  canvas-id="play_canvas" style='z-index: -99;width:100%;height:100%'>
<cover-image  style='width:40px;height:40px;margin-left:10px;margin-top:10px' src='{{playerItem.imagePath}}'></cover-image>
</canvas>
</view>

</template>